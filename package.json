{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "ssr for react",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test:jest": "jest",
    "test:jest:update": "jest --updateSnapshot",
    "lint:js": "eslint --ext .js src",
    "clean": "rimraf dist",
    "build:client:dev": "npm run clean && cross-env NODE_ENV=development SPA_MODE=true babel-node src/scripts/client.js",
    "build:client:prod": "cross-env NODE_ENV=production webpack -p --config webpack/client/index.js",
    "build:router:dev": "cross-env NODE_ENV=development webpack --config webpack/router/index.js",
    "build:router:prod": "cross-env NODE_ENV=production webpack -p --config webpack/router/index.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production ONLY_BUILD=true babel-node src/scripts/index.js",
    "start:spa:server": "http-server-spa dist",
    "start:dev": "npm run clean && cross-env NODE_ENV=development babel-node src/scripts/index.js",
    "start:prod": "npm run clean && cross-env NODE_ENV=production babel-node src/scripts/index.js",
    "analyze:dev": "npm run clean && cross-env NODE_ENV=development ANALYZE=true babel-node src/scripts/index.js",
    "analyze:prod": "npm run clean && cross-env NODE_ENV=production ANALYZE=true babel-node src/scripts/index.js"
  },
  "keywords": [
    "react",
    "ssr",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
